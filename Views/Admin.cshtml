@model List<dynamic>

<table class="table">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Quote</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var quote in Model)
        {
            <tr>
                <td>@quote.FirstName</td>
                <td>@quote.LastName</td>
                <td>@quote.Email</td>
                <td>@quote.Quote</td>
            </tr>
        }
    </tbody>
</table>

